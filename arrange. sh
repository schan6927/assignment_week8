dir="files/"

#cd "%dir" || exit 1

for file in $dir/*.txt; do
    filename=$(basename $file)
    #echo "파일: $(basename "$file")"
    #echo "$(basename "$file")"
    #echo $filename
    for first_small in {a..z}; do
        if [[ $filename == $first_small* ]]; then
            mv $file $first_small
        fi
    done

    for first_big in {A..Z}; do
        if [[ $filename == $first_big* ]]; then
            mv $file ${first_big,}
        fi
    done
done